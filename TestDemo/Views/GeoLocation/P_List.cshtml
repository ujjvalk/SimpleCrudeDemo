@model TestDemo.Models.CustomModels.GeoLocationModel

<input type="hidden" value="@ViewBag.Successmessage" id="SuccessMessage" />
<table id="example" class="display example table-striped table-bordered table-hover" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>GeoLocation Address</th>
            <th>Latitude</th>
            <th>Longiude</th>
            <th style="text-align:center;">Action</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.geoList)
        {
            <tr>
                <td>@item.GeoLocationAddress</td>
                <td>@item.Latitude</td>
                <td>@item.Longiude</td>
                <td style="text-align:center;">
                    @Ajax.ActionLink(" ", "Edit", new { id = item.Id }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "_Form", OnSuccess = "scrolltop();", OnComplete = "edit();" }, new { @Class = "btn btn-info glyphicon glyphicon-edit icon-white", @id = "btnedit" })

                    <a onclick="DeleteRecords('@item.Id', 'Y') " href="javascript:;" class="btn btn-danger glyphicon glyphicon-trash icon-white"></a>
                </td>
            </tr>
        }

    </tbody>
</table>


